# Database
DATABASE_URL=postgres://postgres:postgres@localhost:5432/agentfs

# API
PORT=8787
NODE_ENV=development

# Embeddings (optional for MVP until Phase 2)
OPENAI_API_KEY=
OPENAI_EMBED_MODEL=text-embedding-3-small

# Quotas (example defaults)
QUOTA_WRITES_PER_DAY=5000
QUOTA_EMBED_TOKENS_PER_DAY=2000000
QUOTA_SEARCHES_PER_MINUTE=120

# Admin bootstrap (dev only)
# If set, enables POST /v1/admin/create-key with this token
ADMIN_BOOTSTRAP_TOKEN=
